none main() start
    integer limite << 0;
    integer validade << 0;
    integer valor << 0;

    captura(limite);

    captura(validade);

    captura(valor);

    if(limite > valor and validade > 2021)start
        show(true);
    end else start 
        show(false);
    end

    if(limite < valor or validade <= 2021)start
        show(false);
    end else start 
        show(false);
    end

end